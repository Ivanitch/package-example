{
    "name": "ivanitch/example",
    "description": "A Tutorial for Developing a Package Using Composer",
    "type": "library",
    "authors": [
        {
            "name": "Sergey Ivanov",
            "email": "ivanitch.mail@gmail.com"
        }
    ],
